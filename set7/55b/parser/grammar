%class-name Parser
%filenames parser
%parsefun-source parse.cc
%scanner ../scanner/scanner.h

%token 'm' 
%token 's'
%token DOUBLE 
%token INTEGRAL

//%debug 

%%

input:    
    //empty
|
    input line
;

 line: 
    '\n'
    {
        ACCEPT();
    }
|  
    'm' DOUBLE '\n'
| 
    's' DOUBLE '\n'
| 
    DOUBLE  '\n'
| 
    DOUBLE INTEGRAL '\n'
;

