%class-name Parser
%filenames parser
%parsefun-source parse.cc
%scanner ../scanner/scanner.h

%token DOUBLE 
%token INT

//%debug 

%%

input:    
    //empty
|
    input line '\n'
|
    input '\n'
    {
        std::cout << '\n';      // correctly close the program
        ACCEPT();
    }
;

line: 
    'm' IntOrDouble             // set mean
| 
    's' IntOrDouble             // set sd
| 
    IntOrDouble                 // return score
| 
    IntOrDouble INT             // return score and nObs
;

IntOrDouble:
    DOUBLE
|
    INT
;